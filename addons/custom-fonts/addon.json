{
  "name": "Customizable fonts",
  "description": "Customize the fonts for every part of scratch, including the blocks! Choose a font from fonts.google.com and enter it into the box.",
  "tags": ["theme", "beta"],
  "credits": [
    {
      "name": "ErrorGamer2000",
      "link": "https://scratch.mit.edu/users/ErrorGamer2000/"
    }
  ],
  "info": [
    {
      "type": "warning",
      "text": "Letters in wider fonts will be bunched together. Try not to use excessively large fonts."
    }
  ],
  "enabledByDefault": false,
  "dynamicEnable": true,
  "dynamicDisable": true,
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/*"],
      "runAtComplete": false
    }
  ],
  "settings": [
    {
      "name": "Navigation Bar Font",
      "id": "navfnt",
      "type": "string",
      "default": "Helvetica"
    },
    {
      "name": "Main Font",
      "id": "mainfnt",
      "type": "string",
      "default": "Helvetica"
    },
    {
      "name": "Editor Font",
      "id": "editfnt",
      "type": "string",
      "default": "Helvetica"
    },
    {
      "name": "Block Font",
      "id": "blockfnt",
      "type": "string",
      "default": "Helvetica"
    },
    {
      "name": "Header Font",
      "id": "headfnt",
      "type": "string",
      "default": "Helvetica"
    }
  ],
  "l10n": true
}
